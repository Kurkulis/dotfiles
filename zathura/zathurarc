set default-bg "#14161b"

set font "JetBrains Mono 10"

set inputbar-fg "#14161b"
set inputbar-bg "#A89984"

set recolor true
set recolor-lightcolor "#14161b"
set recolor-darkcolor "#A89984"

set scroll-page-aware "true"
set scroll-full-overlap 0.01
set scroll-step 100

set selection-clipboard clipboard

set statusbar-fg "#A89984"
set statusbar-bg "#14161b"

set window-title-basename true
set statusbar-basename true
